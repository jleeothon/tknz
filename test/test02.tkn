automaton Integer
     state A
        from "0" to "9" goto B
     state B
        from "0" to "9" goto B

automaton String
    start state A
        with '"' goto B
    state B
        from "A" to "B" goto B
        from "a" to "b" goto B
        from "0" to "9" goto B
        with '"' goto C
    stop state C


automaton Identifier
    state state 0
        from "A" to "Z" goto 1
        from "a" to "z" goto 1
    stop state 1
        from "A" to "Z" goto 1
        from "a" to "z" goto 1
